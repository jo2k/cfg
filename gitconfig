[alias]
    last = log -1 HEAD
    one = log --oneline
    adog = log --all --decorate --oneline --graph
    ado = log --all --decorate --oneline
    st = status
    ci = commit
    br = branch
    co = checkout
    unstage = reset HEAD --
    settings = config --global --list
    rlog = reflog
    do-update = update-git-for-windows
